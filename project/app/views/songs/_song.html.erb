<% if false %>
<div id="<%= dom_id song %>">
  <p>
    <strong>名称:</strong>
    <%= song.名称 %>
  </p>

  <p>
    <strong>演唱:</strong>
    <%= song.演唱 %>
  </p>

  <p>
    <strong>作词:</strong>
    <%= song.作词 %>
  </p>

  <p>
    <strong>作曲:</strong>
    <%= song.作曲 %>
  </p>

</div>
<% end %>
<div class="info">
<div class="info-avatar">
    <%= image_tag 'icon5.png' %>
</div>
<div class="info-content">
    <h1 class="info-name"><%= song.名称 %></h1>
    <h5>演唱: <%= song.演唱 %></h5>
    <h5>作词: <%= song.作词 %></h5>
    <h5>作曲: <%= song.作曲 %></h5>
    <h5>类型: <%= song.song_type.名称 if song.song_type %></h5>
    <h5>发布者: <%= song.manipulator.名称 %></h5>
</div>
</div>
